{
  "expo": {
    "name": "R&M",
    "slug": "ram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ram",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "versionCode": 2,
      "runtimeVersion": "1.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.emraksoy.ram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5672b555-ad8c-4fa7-8e8c-dec4f9458c00"
      }
    },
    "owner": "emraksoy",
    "updates": {
      "url": "https://u.expo.dev/5672b555-ad8c-4fa7-8e8c-dec4f9458c00"
    }
  }
}
